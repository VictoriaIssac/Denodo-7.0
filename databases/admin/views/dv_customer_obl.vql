CREATE OR REPLACE VIEW dv_customer_obl AS SELECT "bv_CUSTOMER"."C_CUSTOMER_SK" AS "C_CUSTOMER_SK", "bv_CUSTOMER"."C_CUSTOMER_ID" AS "C_CUSTOMER_ID", "bv_CUSTOMER"."C_CURRENT_CDEMO_SK" AS "C_CURRENT_CDEMO_SK", "bv_CUSTOMER"."C_CURRENT_HDEMO_SK" AS "C_CURRENT_HDEMO_SK", "bv_CUSTOMER"."C_CURRENT_ADDR_SK" AS "C_CURRENT_ADDR_SK", "bv_CUSTOMER"."C_FIRST_SHIPTO_DATE_SK" AS "C_FIRST_SHIPTO_DATE_SK", "bv_CUSTOMER"."C_FIRST_SALES_DATE_SK" AS "C_FIRST_SALES_DATE_SK", "bv_CUSTOMER"."C_SALUTATION" AS "C_SALUTATION", "bv_CUSTOMER".first_name AS first_name, "bv_CUSTOMER"."C_LAST_NAME" AS "C_LAST_NAME", "bv_CUSTOMER"."C_PREFERRED_CUST_FLAG" AS "C_PREFERRED_CUST_FLAG", "bv_CUSTOMER"."C_BIRTH_DAY" AS "C_BIRTH_DAY", "bv_CUSTOMER"."C_BIRTH_MONTH" AS "C_BIRTH_MONTH", "bv_CUSTOMER"."C_BIRTH_YEAR" AS "C_BIRTH_YEAR", "bv_CUSTOMER"."C_BIRTH_COUNTRY" AS "C_BIRTH_COUNTRY", "bv_CUSTOMER"."C_LOGIN" AS "C_LOGIN", "bv_CUSTOMER"."C_EMAIL_ADDRESS" AS "C_EMAIL_ADDRESS", "bv_CUSTOMER"."C_LAST_REVIEW_DATE" AS "C_LAST_REVIEW_DATE", name FROM "bv_CUSTOMER" WHERE "C_LAST_NAME" = name USING PARAMETERS ( name : text);

ALTER VIEW dv_customer_obl
 LAYOUT ("bv_CUSTOMER" = [20, 20, 313, 385]);

